package ciociaria.annunci.it.controller.dto;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.EqualsAndHashCode;

@Data
@AllArgsConstructor
@EqualsAndHashCode(callSuper = false)
public class AnnuncioMotoriDto extends AnnuncioDto {
	private boolean vendiADistanza;
	private String condizioneVeicolo;
	private int annoImmatricolazione;
	private int lunghezzaInMetri;
	private int chilometri;
	private int cilindrata;
	private String alimentazione;
	private String cambio;
	private String colore;
	private int porte;
	private String tipologiaMotore;
	private String marca;
	private String modello;
	
}
