
package it.ciociariaannunci.logic.model.bean;

import java.time.LocalTime;

import org.springframework.data.annotation.Id;
import org.springframework.data.mongodb.core.mapping.Document;
import org.springframework.data.mongodb.core.mapping.Field;

import jakarta.validation.Valid;
import jakarta.validation.constraints.NotEmpty;
import jakarta.validation.constraints.PastOrPresent;
import lombok.Data;
import lombok.NoArgsConstructor;



// ----------- << imports@AAAAAAGGZXlVS2cV0Jw= >>
// ----------- >>


// ----------- << class.annotations@AAAAAAGGZXlVS2cV0Jw= >>
@Data
@NoArgsConstructor
@Document(collection = "segnalazione")
// ----------- >>
public final class Segnalazione {
    // ----------- << attribute.annotations@AAAAAAGGZY0zOmsEEj8= >>
	@Id
    // ----------- >>
    private String id;

    // ----------- << attribute.annotations@AAAAAAGGZYXoB2pmgSA= >>
	@Field("motivo")
	@NotEmpty
    // ----------- >>
    private String motivo;

    // ----------- << attribute.annotations@AAAAAAGGZYXvKWps5/Y= >>
	@Field("messaggio")
    // ----------- >>
    private String messaggio;

    // ----------- << attribute.annotations@AAAAAAGGZYZFMWp2Kxk= >>
    @NotEmpty
    @PastOrPresent
    @Field("data")
    // ----------- >>
    private LocalTime data;

    // ----------- << attribute.annotations@AAAAAAGGcaE9BCWv+fw= >>
    @NotEmpty
    @Valid
    @Field("utente")
    // ----------- >>
    private Utente utente;

// ----------- << class.extras@AAAAAAGGZXlVS2cV0Jw= >>
// ----------- >>
}