
package ciociaria.annunci.it.model.bean;

import java.util.ArrayList;
import java.util.List;

import org.springframework.data.annotation.Id;
import org.springframework.data.mongodb.core.mapping.Field;

import jakarta.validation.Valid;
import jakarta.validation.constraints.Min;
import jakarta.validation.constraints.NotEmpty;
import lombok.AllArgsConstructor;
// ----------- << imports@AAAAAAGGZXlW/2c+nhw= >>
// ----------- >>
import lombok.Data;
import lombok.NoArgsConstructor;

// ----------- << class.annotations@AAAAAAGGZXlW/2c+nhw= >>
@Data
@AllArgsConstructor
@NoArgsConstructor
// @Document(collection = "annnucio")
// ----------- >>
public abstract class Annuncio {
	// ----------- << attribute.annotations@AAAAAAGGZasTV2uXelk= >>
	@Id
	// ----------- >>
	private String id;

	// ----------- << attribute.annotations@AAAAAAGGZatCn2uduy0= >>
	@Field("id_annuncio")
	@NotEmpty
	// ----------- >>
	private Integer idAnnuncio;

	// ----------- << attribute.annotations@AAAAAAGGZatgsmupjcU= >>
	@Field("titolo")
	@NotEmpty
	// ----------- >>
	private String titolo;

	// ----------- << attribute.annotations@AAAAAAGGZatqdWuvgUA= >>
	@Field("descrizione")
	@NotEmpty
	// ----------- >>
	private String descrizione;

	// ----------- << attribute.annotations@AAAAAAGGZaxhzmvq7VQ= >>
	@Field("numero_visite")
	@NotEmpty
	// ----------- >>
	private int numeroVisite;

	// ----------- << attribute.annotations@AAAAAAGGZb2dkmwEZGE= >>
	@Field("prezzo")
	@Min(0)
	// ----------- >>
	private Double prezzo;

	// ----------- << attribute.annotations@AAAAAAGGZb2u12wQLb0= >>
	@Field("tipo_inserzionista")
	@NotEmpty
	// ----------- >>
	private String tipoInserzionista;

	// ----------- << attribute.annotations@AAAAAAGGZb44rGwbfUE= >>
	@Field("natura_annuncio")
	@NotEmpty
	// ----------- >>
	private String naturaAnnuncio;

	// ----------- << attribute.annotations@AAAAAAGGZb5JCmwnrRM= >>
	@Field("foto_annuncio")
	// ----------- >>
	private List<String> fotoAnnuncio;

	// ----------- << attribute.annotations@AAAAAAGGcaTw00pMnzM= >>
	@Field("inserito_da")
	@NotEmpty
	@Valid
	// ----------- >>
	private Utente inseritoDa;

	// ----------- << attribute.annotations@AAAAAAGGc4qQztmAMzg= >>
	@Field("sottocategoria")
	@NotEmpty
	@Valid
	// ----------- >>
	private Sottocategoria sottocategoria;

	// ----------- << attribute.annotations@AAAAAAGGcYDZQ9L0gsY= >>
	@Field("commenti")
	@Valid
	// ----------- >>
	private List<Commenti> commenti;

	// ----------- << attribute.annotations@AAAAAAGGcYuLHORBvOM= >>
	@Field("segnalazione")
	@Valid
	// ----------- >>
	private List<Segnalazione> segnalazione;

	// ----------- << attribute.annotations@AAAAAAGGc9mpAdzd/m8= >>
	@Field("data_annuncio")
	@NotEmpty
	@Valid
	// ----------- >>
	private DataAnnuncio dataAnnuncio;

	// ----------- << attribute.annotations@AAAAAAGGc/PYpydFPsw= >>
	@Field("posizione")
	@Valid
	// ----------- >>
	private Posizione posizione;

	// ----------- << attribute.annotations@AAAAAAGGdI+eDZ9RRc0= >>
	@Field("contatti")
	@Valid
	// ----------- >>
	private Contatti contatti;

	// ----------- << attribute.annotations@AAAAAAGGdJb311zT2Ps= >>
	@Field("stato")
	@NotEmpty
	@Valid
	// ----------- >>
