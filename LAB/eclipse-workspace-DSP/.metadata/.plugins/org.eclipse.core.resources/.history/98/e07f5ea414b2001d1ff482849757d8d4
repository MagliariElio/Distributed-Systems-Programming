// ----------- << class.extras@AAAAAAGGZXlW/2c+nhw= >>
	private final class DataAnnuncio {
		private LocalTime dataPubblicazione;
		private LocalTime dataScadenza;

		public LocalTime getDataPubblicazione() {
			return dataPubblicazione;
		}

		public void setDataPubblicazione(LocalTime dataPubblicazione) {
			this.dataPubblicazione = dataPubblicazione;
		}

		public LocalTime getDataScadenza() {
			return dataScadenza;
		}

		public void setDataScadenza(LocalTime dataScadenza) {
			this.dataScadenza = dataScadenza;
		}
	}

	private final class Posizione {
		private Comune comune;
		private String indirizzo;

		public Comune getComune() {
			return comune;
		}

		public void setComune(Comune comune) {
			this.comune = comune;
		}

		public String getIndirizzo() {
			return indirizzo;
		}

		public void setIndirizzo(String indirizzo) {
			this.indirizzo = indirizzo;
		}
	}

	private final class Contatti {
		private String email;
		private String recapitoTelefonico;

		public String getEmail() {
			return email;
		}

		public void setEmail(String email) {
			this.email = email;
		}

		public String getRecapitoTelefonico() {
			return recapitoTelefonico;
		}

		public void setRecapitoTelefonico(String recapitoTelefonico) {
			this.recapitoTelefonico = recapitoTelefonico;
		}
	}

	private final class Stato {
		private String stato;
		private LocalTime data;

		public String getStato() {
			return stato;
		}

		public void setStato(String stato) {
			this.stato = stato;
		}

		public LocalTime getData() {
			return data;
		}

		public void setData(LocalTime data) {
			this.data = data;
		}
	}

	private final class Modifica {
		private Utente utente;
		private LocalTime data;

		public Utente getUtente() {
			return utente;
		}

		public void setUtente(Utente utente) {
			this.utente = utente;
		}

		public LocalTime getData() {
			return data;
		}

		public void setData(LocalTime data) {
			this.data = data;
		}
	}

	private final class Rimozione {
		private Utente utente;
		private String motivo;
		private LocalTime data;

		public Utente getUtente() {
			return utente;
		}

		public void setUtente(Utente utente) {
			this.utente = utente;
		}

		public String getMotivo() {
			return motivo;
		}

		public void setMotivo(String motivo) {
			this.motivo = motivo;
		}

		public LocalTime getData() {
			return data;
		}

		public void setData(LocalTime data) {
			this.data = data;
		}

	}

